{
    "version": 2,
    "builds": [
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/main.py",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "streamlit run main.py"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
}  